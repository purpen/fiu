<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >

    <RelativeLayout
        android:id="@+id/view_label_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="visible">

        <TextView
            android:id="@+id/view_label_left_name"
            android:layout_width="114dp"
            android:layout_height="24dp"
            android:background="@drawable/label_left"
            android:gravity="center"
            android:maxWidth="114dp"
            android:singleLine="true"
            android:text="亮神智能感应灯"
            android:textColor="@color/yellow_bd8913"
            android:textSize="12sp"/>

        <TextView
            android:id="@+id/view_label_right_price"
            android:layout_width="63dp"
            android:layout_height="24dp"
            android:layout_toRightOf="@id/view_label_left_name"
            android:background="@mipmap/label_price"
            android:gravity="center"
            android:text="¥1288"
            android:textColor="@color/white"
            android:textSize="12sp"/>

        <ImageView
            android:id="@+id/view_label_right_point"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="2dp"
            android:layout_toLeftOf="@id/view_label_right_price"
            android:src="@mipmap/point01"/>

        <ImageView
            android:id="@+id/view_label_delete"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignBottom="@+id/view_label_right_point"
            android:layout_alignEnd="@+id/view_label_right_price"
            android:layout_alignRight="@+id/view_label_right_price"
            android:layout_alignTop="@+id/view_label_right_point"
            android:src="@mipmap/cancel"/>
    </RelativeLayout>

