<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:context="com.taihuoniao.fineix.main.MainActivity">

    <!--<com.taihuoniao.fineix.view.CustomHeadView-->
    <!--android:id="@+id/custom_head"-->
    <!--android:visibility="gone"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content" />-->

    <FrameLayout
        android:id="@+id/activity_main_fragment_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/custom_head"
        android:layout_marginBottom="50dp"/>

    <LinearLayout
        android:id="@+id/activity_main_bottomlinear"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp60"
        android:layout_alignParentBottom="true"
        android:background="@mipmap/main_nav_bg"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_nav0"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_homepagebtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/home_gray"/>

            <TextView
                android:id="@+id/tv_nav0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp2"
                android:gravity="center_horizontal"
                android:text="@string/qing"
                android:textSize="@dimen/sp12"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_nav1"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_findbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/find_gray"/>

            <TextView
                android:id="@+id/tv_nav1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp2"
                android:gravity="center_horizontal"
                android:text="@string/jing"
                android:textSize="@dimen/sp12"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_nav2"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_scenebtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/create"/>

            <TextView
                android:id="@+id/tv_nav2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp2"
                android:gravity="center_horizontal"
                android:text="@string/fiu"
                android:textColor="@color/color_333"
                android:textSize="@dimen/sp12"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_nav3"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_shopbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/shop_gray"/>

            <TextView
                android:id="@+id/tv_nav3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp2"
                android:gravity="center_horizontal"
                android:text="@string/pin"
                android:textSize="@dimen/sp12"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_nav4"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_minebtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/mine_gray"/>

            <TextView
                android:id="@+id/tv_nav4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp2"
                android:gravity="center_horizontal"
                android:text="@string/mine"
                android:textSize="@dimen/sp12"/>
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/activity_main_first_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:src="@mipmap/first_in_wellgood2"
        android:visibility="gone"/>
</RelativeLayout>
