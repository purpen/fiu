<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/activity_main_container"
                tools:context="com.taihuoniao.fineix.main.MainActivity">

    <!--<com.taihuoniao.fineix.view.CustomHeadView-->
    <!--android:id="@+id/custom_head"-->
    <!--android:visibility="gone"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content" />-->

    <FrameLayout
        android:id="@+id/activity_main_fragment_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <LinearLayout
        android:id="@+id/activity_main_bottomlinear"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_alignParentBottom="true"
        android:background="@mipmap/main_nav_bg"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_marginTop="10dp"
            android:id="@+id/ll_nav0"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_homepagebtn"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:src="@mipmap/home_gray"/>

            <TextView
                android:id="@+id/tv_nav0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center_horizontal"
                android:text="@string/qing"
                android:textSize="10sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="10dp"
            android:id="@+id/ll_nav1"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_findbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/find_gray"/>

            <TextView
                android:id="@+id/tv_nav1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:gravity="center_horizontal"
                android:text="@string/jing"
                android:textSize="10sp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_nav2"
            style="@style/MainButtonStyle">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/create_sight"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:gravity="center_horizontal"
                android:text="@string/fiu"
                android:textColor="#999999"
                android:textSize="10sp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_nav3"
            style="@style/MainButtonStyle">

            <ImageView
                android:id="@+id/activity_main_shopbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/shop_gray"/>

            <TextView
                android:id="@+id/tv_nav3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center_horizontal"
                android:text="@string/pin"
                android:textSize="10sp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_nav4"
            style="@style/MainButtonStyle">
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/activity_main_minebtn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/mine_gray"/>
                <TextView
                    android:id="@+id/tv_msg_indicator"
                    android:layout_width="@dimen/dp8"
                    android:layout_height="@dimen/dp8"
                    android:layout_marginRight="@dimen/dp20"
                    android:background="@drawable/msg_indicator"
                    android:layout_alignParentRight="true"
                    android:visibility="gone"
                    />
            </RelativeLayout>

            <TextView
                android:id="@+id/tv_nav4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="center_horizontal"
                android:text="@string/mine"
                android:textSize="10sp"/>
        </LinearLayout>
    </LinearLayout>


    <RelativeLayout
        android:id="@+id/activity_main_first_relative"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <ImageView
            android:id="@+id/activity_main_first_right_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="6dp"
            android:layout_marginTop="3dp"
            android:visibility="gone"/>

        <ImageView
            android:id="@+id/activity_main_first_left_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:layout_marginTop="4dp"
            android:visibility="gone"/>

        <ImageView
            android:id="@+id/activity_main_first_img"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:scaleType="fitXY"
            android:visibility="gone"
            />
    </RelativeLayout>

</RelativeLayout>
